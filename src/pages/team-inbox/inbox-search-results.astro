                                      </div>
                                    </li>
                                    <div class="agent-group-list">
                                      <Radio id="account" title="Account" />
                                      <Radio
                                        id="customerSupport"
                                        title="Customer Support"
                                      />
                                      <Radio
                                        id="escalations"
                                        title="Escalations"
                                      />
                                      <Radio
                                        id="forerunGroup"
                                        title="Forerun group"
                                      />
                                    </div>
                                  </div>
                                </div>
                                <div class="col-lg-6 col-sm-12">
                                  <!-- select an agent -->
                                  <div class="agent-group">
